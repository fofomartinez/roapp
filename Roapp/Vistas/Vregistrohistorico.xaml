<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Roapp.Vistas.Vregistrohistorico">
    <ContentPage.Content>
        <StackLayout>
            <Entry Placeholder="Fecha y hora" x:Name="txtfecha"/>
            <Entry Placeholder="Ubicacion" x:Name="txtubicacion"/>
            <Entry Placeholder="Tipo" x:Name="txttipo"/>

            <Button Text="Guardar" x:Name="btnGuardar"
                    Clicked="btnGuardar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>